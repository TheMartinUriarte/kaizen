<div class="row">
  <div class="col-md-2 col-sm-2 col-xs-4 full-height">
    <%= render partial: '/partials/nav' %>
  </div>
  <div class="col-md-10 col-sm-10 col-xs-8 ">
    <%= render partial: '/partials/userinfo' %>

    <div class="col-lg-3">
      <div class="panel panel-default">
        <% @manager.developers.each do |developer| %>
        <%= developer.name %>
        <% end %>
      </div>
    </div>
    Projects:<br>
    <% @projects.each do |project| %>
      <p>
        <%= link_to project.title, "/projects/#{project.title}" %><br>
        This project is due on <%= project.deadline %>!!!<br>
        I'm a paragraph describein your project: <%= project.description %><br>
      </p>
    <% end %>

      <%=button_to "Add New Skill", '/skills/new', method: :get, class: "btn btn-info"%>
      <br>
      <%=button_to "Add New Developer", '/team/new', method: :get, class: "btn btn-info"%>
      <br>
      <%=button_to "Add New Project", '/projects/new', method: :get, class: "btn btn-info"%>
  </div>
</div>
